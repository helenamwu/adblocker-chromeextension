{
    "manifest_version": 2,
    "name": "Chrome AdBlocker",
    "description": "Blocking ads.",
    "version": "0.0.1",
    "author": "<AUTHOR_NAME>",
    "browser_action": {
        "default_title": "Chrome AdBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/",
        "https://www.reddit.com/",
        "https://www.twitter.com/"
    ],
    "background": {
        "scripts": [
            "extension.js"
        ]
    }
}